{
  "mcpServers": {
    "git": {
      "command": "uvx",
      "args": [
        "mcp-server-git"
      ],
      "env": {},
      "disabled": false,
      "autoApprove": [
        "git_status",
        "git_diff",
        "git_log",
        "git_diff_unstaged",
        "git_diff_staged"
      ]
    }
  }
}